{"data":{"properties":{"created_at":1631290287000,"events":{},"identifier":"feedback/progress","modified_at":1631290287000,"properties":{"Progress":[{"name":"palette","description":"<p>Alters the displayed color scheme.</p>\n","types":["auto","inverse","inherit","accent","dark","light","alert","affirmative","negative"]},{"name":"shape","description":"<p>Changes the shape of the <code>Progress</code>.</p>\n","types":["circle"]},{"name":"size","description":"<p>Renders the <code>Progress</code> at a different sizes.</p>\n","types":["tiny","small","medium","large","huge"]},{"name":"value","description":"<p>Changes the percentage value <em>(0.0...1.0)</em> that is rendered.</p>\n","types":["number","string","undefined"]}]},"sections":[{"identifier":"progress","level":1,"text":"Progress"},{"identifier":"imports","level":2,"text":"Imports"},{"identifier":"shape","level":2,"text":"Shape"},{"identifier":"palette","level":2,"text":"Palette"},{"identifier":"size","level":2,"text":"Size"},{"identifier":"indeterminate","level":2,"text":"Indeterminate"}],"snippets":[{"identifier":"progress-preview","script":"<script>\n    import {\n        Button,\n        Progress,\n    } from \"@kahi-ui/framework\";\n\n    let value = 0.5;\n</script>\n\n<Progress shape=\"circle\" {value} />\n<Progress {value} />\n\n<Button\n    palette=\"negative\"\n    on:click={() =>\n        (value = Math.max(0, value - 0.05))}\n>\n    -0.05\n</Button>\n\n<Button\n    palette=\"affirmative\"\n    on:click={() =>\n        (value = Math.min(1, value + 0.05))}\n>\n    +0.05\n</Button>","syntax":"svelte","title":"Progress Preview"},{"identifier":"progress-imports","script":"<script>\n    import {Progress} from \"@kahi-ui/framework\";\n</script>","syntax":"html","title":"Progress Imports"},{"identifier":"progress-shape","script":"<script>\n    import {Progress} from \"@kahi-ui/framework\";\n</script>\n\n<Progress shape=\"circle\" value={0.5} />\n<Progress value={0.5} />","syntax":"svelte","title":"Progress Shape